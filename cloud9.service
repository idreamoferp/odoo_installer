[Unit]
Description=Cloud 9 IDE
After=network.target

[Service]
Type=simple
SyslogIdentifier=c9sdk
PermissionsStartOnly=true
User=root
ExecStart=/usr/bin/node /opt/c9sdk/server.js -p 3000 -l 0.0.0.0 -a debian:temppwd --collab -w /var/lib/cloud9
StandardOutput=journal+console

[Install]
WantedBy=multi-user.target

